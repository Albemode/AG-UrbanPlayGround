<h2 class="title">Events Index</h2>
<div id="events">

			<% @events.each do |event| %>

				<%= event.id %>
				<%= event.title %>
        <%= event.description %>
        <%= event.start_at %>
        <%= event.location %>

				<%= link_to 'Show', event_path(event.id) %>

			<% end %>
</div>
